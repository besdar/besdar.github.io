{
  "name": "besdar.github.io",
  "version": "2.0.0",
  "author": "besdar",
  "description": "Personal resume",
  "scripts": {
    "dev": "rm -rf ./dist/* && cp 'public/Dmitry Beskov.pdf' './dist/Dmitry Beskov.pdf' && parcel ./public/index.html",
    "build": "rm -rf ./dist/* && parcel build ./public/index.html",
    "deploy": "gh-pages -d dist",
    "check-tests": "jest",
    "check-js": "eslint --cache --ext .ts,.tsx ./src",
    "check-styles": "stylelint \"./src/**/*.css\"",
    "check-types": "tsc",
    "project-checking": "concurrently \"npm:check-*\"",
    "update-css-types": "tcm src --namedExports"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.3.0",
    "@parcel/transformer-webmanifest": "^2.3.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "parcel": "^2.3.0",
    "postcss-modules": "^4.3.0",
    "stylelint": "^14.4.0",
    "stylelint-config-standard": "^25.0.0",
    "ts-jest": "^27.1.3",
    "typed-css-modules": "^0.7.0",
    "typescript": "^4.5.5",
    "typescript-plugin-css-modules": "^3.4.0"
  }
}
